<%- include("partials/header.ejs") %>
<form class="login" action="/login" method="POST">
   <h2>Welcome, User!</h2>
   <p>Please log in</p>
   <input type="text" placeholder="User Name"  name="email" />
   <input type="password" placeholder="Password" name="password" />
